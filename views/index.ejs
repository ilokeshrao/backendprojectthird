<body data-spy="scroll" data-target=".main-nav">


	<section id="section-banner">
		<div class="container">
			<div class="row">
				<div class="banner-content wow fadeInRight">
					<h2 class="title">
						Hello,<br />
						<span>
							I'm <%=bannerRecord.name%> <br />
								a <%=bannerRecord.desc%>,
									From india. </span>
					</h2>
					<a href="#section-profile" class="btn  btn-default">Learn More</a>
				</div>
			</div>
		</div>
	</section>

	<!-- section menu start -->
	<section class="section-menu">
		<div class="navbar navbar-default main-nav" role="navigation">
			<div class="container">
				<div class="navbar-header">
					<button class="navbar-toggle collapsed" data-target="#bs-example-navbar-collapse-1"
						data-toggle="collapse">
						<span class="sr-only"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>

					<a href="#" class="navbar-brand">Lokesh ydv</a>
				</div> <!-- navbar-header end -->
				<!-- main nav  -->

				<div class="collapse navbar-collapse navigation" id="bs-example-navbar-collapse-1" role="navigation">
					<ul class="nav navbar-nav navbar-right">
						<li class="active"><a href="#section-banner">Home </a></li>
						<li><a href="#section-profile">About me </a></li>
						<li><a href="#section-skill">Skills </a></li>
						<li><a href="#section-resume">Resume </a></li>
						<li><a href="#section-testimonial">Testimonials </a></li>
						<li><a href="#section-portfolio">Portfolio </a></li>
						<li><a href="#section-contact">Contact me </a></li>
					</ul>
				</div><!-- /.navbar-collapse -->
			</div><!-- /.container-fluid -->
		</div>
	</section>
	<!-- section menu end -->
	<section id="section-overview" class="section-padding">
		<div class="container">
			<div class="row">
				<%courseRecord.forEach((result)=>{%>

					<div class="service-wrapper">
						<div class="service-inner wow fadeInRight" data-wow-delay=".2s">
							<img src="/upload/<%=result.icon%>" alt="">
							<div class="service-box">
								<h3>
									<%=result.title%>
								</h3>
								<p>
									<%=result.desc%>
								</p>
							</div>
						</div> <!-- service -inner  -->
					</div>
					<%})%>


			</div><!-- row end -->


		</div>
	</section>
	<!-- section overview end -->

	<!-- section profile start -->

	<section id="section-profile" class="section-padding">
		<div class="profile-bg visible-md visible-lg"></div>
		<div class="container">
			<div class="row">
				<div class="col-md-7 col-sm-12 pull-right">
					<div class="profile-desc wow fadeInRight">
						<h2 class="section-title uppercase">Hello, My Name is Lokesh.</h2>
						<br />
						<p> I am a full-stack developer is a professional who possesses the skills and expertise to work
							on both the front-end (client-side) and back-end (server-side) of web applications or
							software. They are proficient in multiple programming languages, frameworks, and
							technologies, allowing them to handle various aspects of a project. Here's an overview of
							what it means to be a full-stack developer:

						</p>
						<br />


						<p class="text-grey animated bounceInUp" style="opacity: 1;">
							Front-End Developers: Focus on creating the user interface and user experience of websites
							or applications using HTML, CSS, and JavaScript.

						</p>
						<br />
						<p class="text-grey animated bounceInUp" style="opacity: 1;">
							Back-End Developers: Deal with the server-side logic and databases, ensuring the
							functionality and data management of web applications.
						</p>
					</div>


				</div>
			</div> <!-- row end -->
		</div><!-- container end -->
	</section>
	<!-- section profile end -->

	<!-- section skill start -->

	<section id="section-skill" class="section-padding">
		<div class="container">
			<div class="row">

				<div class="col-md-12">
					<h2 class="heading-title">My skills</h2>
				</div>
				<div class="col-md-6 col-sm-6 wow fadeInLeft">
					<div class="skill-desc">
						<%skillRecord.forEach((result)=>{%>
							<p>
								<%=result.desc%>
							</p>

							<%})%>
					</div>
				</div>

				<div class="col-md-6 col-sm-6 wow fadeInRight">
					<%skillRecord.forEach((result)=>{%>
						<div class="skill-box wow fadeInDown" data-wow-delay=".2s">
							<label>
								<%=result.skill%>
							</label>
							<div class="progress">
								<div class="progress-bar" role="progress-bar" aria-valuemin="0"
									aria-valuenow="<%=result.strength%>" aria-valuemax="100"
									style="width: <%=result.strength%>%">
									<%=result.strength%>%
								</div>
							</div>
						</div>
						<%})%>
				</div>
			</div>
		</div>
	</section>
	<!-- section skill End -->

